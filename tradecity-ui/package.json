    "format": "prettier --write src/",
    "generate": "graphql-codegen --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@orama/orama": "^2.0.0-beta.7",
    "@types/lodash": "^4.14.202",
    "@vue/apollo-composable": "^4.0.0-beta.12",
    "chart.js": "^4.4.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^0.13.0",
    "@babel/plugin-proposal-explicit-resource-management": "^7.23.9",
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@parcel/watcher": "^2.3.0",
    "@rushstack/eslint-patch": "^1.3.3",
